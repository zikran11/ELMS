<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.LMS">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app_elms"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_app_elms_round"
        android:supportsRtl="false"
        android:theme="@style/Theme.ELMS">

        <activity
            android:name="com.app.LMS.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.LMS.PanelSelection"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.app.LMS.Manager.ManagerLogin"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.app.LMS.Manager.ManagerPanel"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.app.LMS.Manager.AddEmployee"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.app.LMS.Manager.ViewEmployee"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.app.LMS.Manager.EvaluateLeave"
            android:screenOrientation="portrait"/>

        <activity
            android:name="com.app.LMS.Employee.EmployeeLogin"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.app.LMS.Employee.EmployeePanel"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.app.LMS.Employee.LeaveStatus"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Employee.RequestLeave"
            android:screenOrientation="portrait"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>